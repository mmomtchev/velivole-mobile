{
  "expo": {
    "name": "velivole.fr / meteo.guru",
    "slug": "velivole",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icons/velivole.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./icons/velivole.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "fr.velivole.reactnative",
      "supportsTablet": true
    },
    "android": {
      "package": "fr.velivole.reactnative",
      "adaptiveIcon": {
        "foregroundImage": "./icons/velivole.svg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./icons/velivole.svg"
    }
  }
}
