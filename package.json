{
  "name": "velivole-react-native",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "i18n:extract": "react-gettext-parser -c react-gettext-parser.config.js --output build/messages.pot 'src/{*.ts,*.tsx}'",
    "i18n:gen": "po2json -f mf lang/fr.po build/fr.json && po2json -f mf --fallback-to-msgid build/messages.pot build/en.json",
    "i18n": "yarn i18n:extract && yarn i18n:gen",
    "postinstall": "patch-package && mkdir -p build && yarn i18n",
    "lint": "eslint --fix src/*.ts*"
  },
  "dependencies": {
    "@expo/webpack-config": "~0.16.2",
    "@mmomtchev/react-native-settings": "^1.0.6",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@types/uuid": "^8.3.4",
    "expo": "~45.0.0",
    "expo-asset": "~8.5.0",
    "expo-device": "~4.2.0",
    "expo-file-system": "~14.0.0",
    "expo-linking": "~3.1.0",
    "expo-localization": "~13.0.0",
    "expo-location": "~14.2.2",
    "expo-notifications": "^0.15.2",
    "expo-splash-screen": "~0.15.1",
    "expo-sqlite": "~10.2.0",
    "expo-status-bar": "~1.3.0",
    "expo-task-manager": "~10.2.1",
    "expo-updates": "~0.13.2",
    "i18n-js": "^3.9.2",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.68.2",
    "react-native-autocomplete-dropdown": "^2.0.1",
    "react-native-get-random-values": "^1.8.0",
    "react-native-root-toast": "https://github.com/mmomtchev/react-native-root-toast#deprected-prop-types",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.17.7",
    "react-native-webview": "11.18.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@mmomtchev/expo-android-localized-app-name": "^1.0.0",
    "@types/i18n-js": "^3.8.2",
    "@types/react": "~17.0.21",
    "@types/react-dom": "^18.0.4",
    "@types/react-native": "~0.66.13",
    "@types/react-native-autocomplete-input": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eas-cli": "^0.53.0",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "expo-cli": "^5.4.9",
    "expo-dev-client": "^0.9.6",
    "patch-package": "^6.4.7",
    "po2json": "^0.4.5",
    "postinstall-postinstall": "^2.1.0",
    "react-gettext-parser": "^1.16.0",
    "react-native-svg-transformer": "^1.0.0",
    "typescript": "~4.3.5",
    "webpack-dev-server": "~3.11.0"
  },
  "license": "GPL-3.0"
}
